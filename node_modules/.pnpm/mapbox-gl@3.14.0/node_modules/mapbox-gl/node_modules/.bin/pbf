#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/system-4/PycharmProjects/open_ai_news/pizza_example/node_modules/.pnpm/pbf@4.0.1/node_modules/pbf/bin/node_modules:/home/system-4/PycharmProjects/open_ai_news/pizza_example/node_modules/.pnpm/pbf@4.0.1/node_modules/pbf/node_modules:/home/system-4/PycharmProjects/open_ai_news/pizza_example/node_modules/.pnpm/pbf@4.0.1/node_modules:/home/system-4/PycharmProjects/open_ai_news/pizza_example/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/system-4/PycharmProjects/open_ai_news/pizza_example/node_modules/.pnpm/pbf@4.0.1/node_modules/pbf/bin/node_modules:/home/system-4/PycharmProjects/open_ai_news/pizza_example/node_modules/.pnpm/pbf@4.0.1/node_modules/pbf/node_modules:/home/system-4/PycharmProjects/open_ai_news/pizza_example/node_modules/.pnpm/pbf@4.0.1/node_modules:/home/system-4/PycharmProjects/open_ai_news/pizza_example/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../pbf@4.0.1/node_modules/pbf/bin/pbf" "$@"
else
  exec node  "$basedir/../../../../../pbf@4.0.1/node_modules/pbf/bin/pbf" "$@"
fi
